{
  "name": "unplugin-vue-images",
  "type": "module",
  "version": "0.0.5",
  "description": "Victor Bo's Unplugin Template.",
  "license": "MIT",
  "homepage": "https://github.com/vtrbo/unplugin-vue-images#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vtrbo/unplugin-vue-images.git"
  },
  "bugs": {
    "url": "https://github.com/vtrbo/unplugin-vue-images/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.js"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "require": "./dist/webpack.cjs",
      "import": "./dist/webpack.js"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "require": "./dist/rollup.cjs",
      "import": "./dist/rollup.js"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "require": "./dist/esbuild.cjs",
      "import": "./dist/esbuild.js"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "require": "./dist/nuxt.cjs",
      "import": "./dist/nuxt.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "require": "./dist/types.cjs",
      "import": "./dist/types.js"
    },
    "./resolver": {
      "types": "./dist/resolver.d.ts",
      "require": "./dist/resolver.cjs",
      "import": "./dist/resolver.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist",
    "types",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "play": "npm -C playground run dev",
    "example:build": "npm -C examples/vite-vue3 run build",
    "example:dev": "npm -C examples/vite-vue3 run dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "debug": "^4.3.4",
    "fast-glob": "^3.3.1",
    "local-pkg": "^0.4.3",
    "unplugin": "^1.4.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.8",
    "@types/node": "^20.4.10",
    "@vtrbo/eslint-config": "^0.3.10",
    "@vtrbo/utils": "^0.3.6",
    "@vue/compiler-sfc": "^3.3.4",
    "bumpp": "^9.1.1",
    "chalk": "^5.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.47.0",
    "esno": "^0.17.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1",
    "rollup": "^3.28.0",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vitest": "^0.34.1",
    "webpack": "^5.88.2"
  }
}
